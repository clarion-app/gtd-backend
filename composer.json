            "providers": [
                "ClarionApp\\GettingThingsDone\\GettingThingsDoneServiceProvider"
            ]
        },
        "clarion": {
            "app-name": "clarion-app/gtd",
            "description": "Provides operations to implement the Getting Things Done organization system."
        }
